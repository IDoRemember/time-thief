.detail-background {
    background: #fff;
}
.detail-img-box {
    position: relative;
    width:100%;
    height: 350rpx;
    text-align: center;
    overflow: hidden;
}
.detail-img {
    width:100%;
    position: absolute;
    left: 0;
    bottom:0;
    z-index: 1;
}
.detail-title {
    font-weight: 500;
    color:#fff;
    margin-top: -100rpx;
    margin-left: 20rpx;
    z-index: 1000;
    font-size: 32rpx;
    align-self: flex-start;
}
.detail-date {
    width: 90%;
    font-weight: 500;
    color:#fff;
    margin-top:-50rpx;
    margin-left: 20rpx;
    z-index: 1000;
    font-size: 24rpx;
    align-self: flex-start;
}
.detail-content {
    margin: 110rpx 0  10rpx;
    width: 95%;
    font-weight: 500;
    line-height: 40rpx;
    color: #1f1f1f;
    font-size: 28rpx;
}
.detail-strategy {
    width: 30rpx;
    height: 30rpx;
}
.detail-address {
    color:#749ccf;
    font-size: 24rpx;
}
.detail-strategy-box {
    width:95%;
}